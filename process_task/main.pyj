from wsclient import WsClient


class MyClient(WsClient):
    url = '/progress/'
    auto_dispatch = True

    def on_open(evt):
        print('web socket open')

    def on_progress(evt, obj):
        print(obj)


jq = window.jQuery
client = MyClient()

jq('button').on('click', def():
    jq.get('/start-task/')
)
